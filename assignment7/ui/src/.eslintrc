{
  "extends": "airbnb",
  "env": {
    "import/extensions": ["error", "always", { "ignorePackages": true } ],
    "browser": true
  },
  rules: {
    "react/prop-types": "off",
    "no-console": "off",
   	"no-tabs": 0,
   	"no-mixed-spaces-and-tabs": 0,
  	"no-unused-vars" : 0,
   	"max-len": 0,
   	"no-alert": 0,
   	"consistent-return": 0,
   	"react/destructuring-assignment": 0
  }
}